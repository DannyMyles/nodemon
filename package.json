{
  "name": "back-auth",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "npx nodemon ./dist/main.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts,.js && tsc -p tsconfig.json",
    "lint:fix": "eslint src/** --fix --ext .ts,.js && tsc -p tsconfig.json",
    "prettify": "prettier --write .",
    "dev": "tsc && env-cmd -f .env nodemon ./dist/main.js",
    "migration:init:role": "npx sequelize-cli model:generate --name role",
    "migration:role:up": "npx sequelize-cli db:migrate",
    "migration:role:down": "npx sequelize-cli db:migrate:undo",
    "seeders:role:up": "npx sequelize-cli db:seed:all",
    "seeders:role:down": "npx sequelize-cli db:seed:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.1.2",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
